#!/bin/bash -ex

set_cloud_provider() {
  cloud_provider=""
<% if_p("cloud-provider.type") do | provider_type | %>
  cloud_provider="<%= provider_type %>"
<% end %>
}

<% if_p("cloud-provider.gce.service_key") do | v | %>
  export GOOGLE_APPLICATION_CREDENTIALS=/var/vcap/jobs/cloud-provider/config/service_key.json
<% end %>
